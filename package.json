    "@mercuryworkshop/wisp-js": "^0.3.3",
    "@types/node": "^22.9.4",
    "astro": "^4.16.14",
    "chalk": "^5.3.0",
    "fastify": "^5.1.0",
    "lucide-astro": "^0.460.0",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
